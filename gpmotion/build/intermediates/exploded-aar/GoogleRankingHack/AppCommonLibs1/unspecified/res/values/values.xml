    <declare-styleable name="drop_down_list_attr"><attr format="boolean" name="isDropDownStyle"/><attr format="boolean" name="isOnBottomStyle"/><attr format="boolean" name="isAutoLoadOnBottom"/></declare-styleable>
    <dimen name="drop_down_list_footer_button_height">48dp</dimen>
    <dimen name="drop_down_list_footer_button_margin_left">8dp</dimen>
    <dimen name="drop_down_list_footer_progress_bar_height">36dp</dimen>
    <dimen name="drop_down_list_header_padding_bottom">15dp</dimen>
    <dimen name="drop_down_list_header_padding_top">12dp</dimen>
    <dimen name="drop_down_list_header_progress_bar_height">36dp</dimen>
    <dimen name="drop_down_list_header_release_min_distance">20dp</dimen>
    <integer name="card_flip_time_full">300</integer>
    <integer name="card_flip_time_half">150</integer>
    <integer name="half_slide_up_down_duration">150</integer>
    <integer name="slide_up_down_duration">200</integer>
    <integer name="slide_up_down_final_value">150</integer>
